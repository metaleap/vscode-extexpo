{
    "name": "vscode-extexpo",
    "displayName": "VScode Extensions Expo",
    "description": "reference/minimal implementations of nigh-all types of VScode extensions in a single pkg",
    "version": "1.0.0",
    "publisher": "metaleap",
    "engines": {
        "vscode": "^1.10.2"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:expo.demoInfoMsg"
    ],
    "main": "./out/src/extexpo",
    "contributes": {
        "commands": [{
            "command": "expo.demoInfoMsg",
            "title": "EXPO: window.showInformationMessage"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.2.1",
        "vscode": "^1.1.0",
        "mocha": "^3.2.0",
        "@types/node": "^7.0.8",
        "@types/mocha": "^2.2.40"
    }
}
